{
  "name": "pluggy-connect-sdk",
  "version": "2.9.2",
  "description": "Pluggy Connect SDK",
  "main": "dist/main/index.js",
  "typings": "dist/main/index.d.ts",
  "module": "dist/module/index.js",
  "repository": "https://github.com/pluggyai/pluggy-connect",
  "license": "MIT",
  "keywords": [],
  "author": "Pluggy Engineering <dx@pluggy.ai>",
  "scripts": {
    "build": "rimraf dist && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "build:standalone": "npx rollup -c",
    "copy-cdn": "npm run build:standalone && cp dist/bundle/index.js ../cdn/pluggy-connect/latest/pluggy-connect.js && mkdir ../cdn/pluggy-connect/v$npm_package_version && cp ./dist/bundle/index.js ../cdn/pluggy-connect/v$npm_package_version/index.js",
    "lint": "prettier \"src/**/*.ts\" --list-different && eslint src --ext .ts",
    "lint:fix": "prettier \"src/**/*.ts\" --write && eslint src --ext .ts --fix ",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "zoid": "9.0.63"
  },
  "peerDependencies": {
    "pluggy-js": ">= 0.16.0"
  },
  "peerDependenciesMeta": {
    "pluggy-js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "cordova-plugin-inappbrowser": "^5.0.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rollup": "^2.36.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-node": "^9.0.0",
    "tslib": "^1.14.1",
    "typescript": "^4.0.2"
  },
  "files": [
    "dist/main",
    "dist/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
