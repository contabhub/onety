{
  "name": "react-pluggy-connect",
  "version": "2.10.2",
  "description": "React wrapper for Pluggy Connect SDK",
  "main": "dist/main/index.js",
  "typings": "dist/main/index.d.ts",
  "module": "dist/module/index.js",
  "repository": "https://github.com/pluggyai/react-pluggy-connect",
  "license": "MIT",
  "keywords": [],
  "author": "Tomas Milar <tomas@pluggy.ai>",
  "scripts": {
    "build": "rimraf dist && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "build:bundle": "npx rollup -c",
    "lint": "tsc --noEmit && eslint src --ext \"ts\"",
    "lint:fix": "npm run lint -- --fix",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "pluggy-connect-sdk": "^2.8.1"
  },
  "peerDependencies": {
    "pluggy-js": ">= 0.16.0",
    "react": ">= 16.10.0",
    "react-dom": ">= 16.10.0"
  },
  "peerDependenciesMeta": {
    "pluggy-js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rollup": "^2.36.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-node": "^9.0.0",
    "tslib": "^1.14.1",
    "typescript": "^4.0.2"
  },
  "files": [
    "dist/main",
    "dist/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
